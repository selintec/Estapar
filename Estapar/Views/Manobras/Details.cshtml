@model Estapar.Models.Manobra

@{
    ViewData["Title"] = "Detalhes";
}

<h1>Detalhes</h1>

<div>
    <h4>Manobra</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Manobrista)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Manobrista.nom_manobrista)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Carro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Carro.nom_marca)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.dat_manobra)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.dat_manobra)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.tip_manobra)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.tip_manobra)
        </dd>
    </dl>
